{
  "name": "homework-diary",
  "version": "0.2.0",
  "description": "Homework management application",
  "keywords": [
    "Internet Computer",
    "Motoko",
    "JavaScript",
    "Canister"
  ],
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "scripts": {
    "build": "webpack",
    "prebuild": "dfx generate",
    "start": "webpack serve --mode development --env development",
    "deploy:local": "dfx deploy --network=local",
    "deploy:ic": "dfx deploy --network=ic",
    "generate": "dfx generate HomeworkDiary_backend",
    "format": "yarn run rome format .",
    "format:fix": "yarn run rome format . --write",
    "lint": "yarn run rome check .",
    "lint:fix": "yarn run rome check . --apply"
  },
  "dependencies": {
    "@dfinity/agent": "^0.15.6",
    "@dfinity/candid": "^0.15.6",
    "@dfinity/principal": "^0.15.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chakra-ui/react": "^2.6.1",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "assert": "2.0.0",
    "buffer": "6.0.3",
    "copy-webpack-plugin": "^11.0.0",
    "dotenv": "^16.0.3",
    "events": "3.3.0",
    "framer-motion": "^10.12.12",
    "html-webpack-plugin": "5.5.0",
    "lint-staged": "^13.2.2",
    "process": "0.11.10",
    "rome": "^12.1.2",
    "simple-git-hooks": "^2.8.1",
    "stream-browserify": "3.0.0",
    "terser-webpack-plugin": "^5.3.3",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "util": "0.12.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.8.1"
  },
  "engines": {
    "node": "^12 || ^14 || ^16 || ^18"
  },
  "browserslist": [
    "last 2 chrome version",
    "last 2 firefox version",
    "last 2 safari version",
    "last 2 edge version"
  ]
}